using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Aufgabe_2
{
  internal class Program
  {
    static void Main(string[] args)
    {

      int zahl = 0;

      Console.Write("zahl: ");
      zahl = Convert.ToInt32(Console.ReadLine());

      if (zahl >= 0 && zahl <= 100)
      {
        if (zahl % 10 == 0)
        {
          Console.WriteLine("Die eingegebene Zahl" +" " +zahl+ " " + "Liegt in der Zehner Reihe");
        }
        else
        {
          Console.WriteLine("Die eingegebene Zahl" + " " +zahl+ " "+ "Liegt nicht in der Zehner Reihe");
        }
      }
      else
      {
        Console.WriteLine("Die Zahl ist nicht im bereich von 0-100");
      }
      Console.ReadLine();
    }
  }
}
